{
  "Application": {
    "CookieDomain": "coxautoconnect.com",
    "RequestHost": "api.uat.cmr.coxautoconnect.com"
  },
  "NewRelic": {
    "AgentEnabled": "true"
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.NewRelic.Logs",
      "Serilog.Sinks.Seq",
      "NewRelic.LogEnrichers.Serilog"
    ],
    "Enrich": [
      "WithNewRelicLogsInContext"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Default": "Information",
        "Microsoft": "Information",
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "NewRelicLogs",
              "Args": {
                "applicationName": "cmr-uat-payment-api",
                "licenseKey": "{LicenseKey}",
                "endpointUrl": "https://log-api.newrelic.com/log/v1"
              }
            }
          ]
        }
      },
      {
        "Name": "Console"
      }
    ]
  },
  "AWS": {
    "DynamoDb": {
      "ServiceUrl": "https://dynamodb.eu-west-1.amazonaws.com",
      "TableNamePrefix": "cmr-uat-"
    }
  }
}
